*{
	margin: 0;
	padding: 0;
}
body{
	overflow: hidden;
	margin: 0;
	padding: 0;
	}
a{
	text-decoration: none;		
	}
a:hover{
	text-decoration: none;
	}
.navbar-fixed-top{
	position: fixed;
    width: 100%;
    height: 45px;
    background: rgba(0, 0, 0, 0) url(../img/top13.png) repeat-x scroll 0 0;
    top: 0px;
    left: 0px;
    right: 0px;
}
.navbar-fixed-top p{
	text-align: center;
    font-size: 30px;
    font-weight: bold;
    position: relative;
  
    color: #ffffff;
}

.container-fluid {
    padding-right: 15px;
    padding-left: 15px;
    margin-right: auto;
    margin-left: auto;
    margin-top: 46px;
}


.left_top {
    background: url(../img/left_top_bg.jpg) repeat-x 0 0;
    width: 100%;
    display: inline-block;
}


 .left_top_back {
    text-decoration: none;
    width: 118px;
    height: 38px;
    line-height: 38px;
    float: left;
    display: block;
    color: #215278;
    text-align: center;
    margin-left: 20px;
}

.accordion h1 {
   
    cursor: pointer;
    font-size: 16px;
    font-weight: normal;
    margin: 0 !important;
    text-align: center;
}
.accordion h1 a {
    text-decoration: none;
    color: #215278;
}
.accordion #remasg a:hover{
	cursor: pointer;
	color: #ffffff;
	}	

ul, li {
    padding: 0;
    margin: 0px 0 0px 0px!important;
    list-style: none;
    text-align: center;
    color: #000000;
    /* border: 1px solid #cccccc; */
}
ul, li:hover {
    padding: 0;
    margin: 0px 0 0px 0px!important;
    list-style: none;
    text-align: center;
    color: #0088CC;
}
.tab-header li .close{
     float: left;
    display: block;
    position: relative;
    right: -74px;
    top: -18px;
    font-weight: 600;
    cursor: pointer;
    width: 10px;
    height: 10px;
    color: #ccc;
	}
.tab-header li .close:hover{
     float: left;
    display: block;
    position: relative;
    right: -74px;
    top: -18px;
    font-weight: 600;
    cursor: pointer;
    color: red;
	}	
.tab-header li .close1{
     float: left;
    display: block;
    position: relative;
    right: -74px;
	top: -69px;		
    font-weight: 600;
    cursor: pointer;
    width: 10px;
    height: 10px;
    color: #ccc;
	}	
.tab-header li .close1:hover{
     float: left;
    display: block;
    position: relative;
    right: -74px;
    top: -69px;
    font-weight: 600;
    cursor: pointer;
    color: red;
	}

.accordion .itme{
	border: 1px solid #BBBBBB;
	}

