/*上传文件按钮*/
.file {
            position: relative;
            display: inline-block;
            background: #eee;
            border: 1px solid #ccc;
            border-radius: 4px;
            padding: 4px 8px;
            color: #444;
            text-decoration: none;
        }
        .file input{
            position: absolute;
            opacity: 0;
            width: 71px;
			top: -1px;
			left: 0px;
			height: 30px;
        }
        .file:hover {
            color: #444;
        }
        .file input:hover {
            cursor:pointer;
        }