.textarea{
	height: 60px;
	width: 250px;
	resize: none;
	transition: all 0.30s ease-in-out;
	-webkit-transition: all 0.30s ease-in-out;
	-moz-transition: all 0.30s ease-in-out;
	-moz-border: rgba(81, 203, 238, 1) 1px solid;
	*border: rgba(81, 203, 238, 1) 1px solid;
	_border: rgba(81, 203, 238, 1) 1px solid;
	border: rgba(81, 203, 238, 1) 1px solid;
	*border-radius: 3px;
	_border-radius: 3px;
	border-radius: 3px;
	-moz-border-radius: 3px;
	outline: none;
}
	    .position{position: relative;
