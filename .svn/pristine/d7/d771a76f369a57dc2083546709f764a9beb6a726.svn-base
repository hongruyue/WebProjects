/*......banner........*/
.banner{ width:100%; height:340px; float:left;}
#wrap 
  { 
   height: 340px; 
   width: 100%; 
   overflow: hidden; 
   position: relative; 
 
  }
  #wrap ul 
  { float:right;
   list-style: none; 
   margin-top:290px;
   margin-right:100px;
  


  }
  #wrap li 
  { 
   margin-left:2px; 
   opacity: .6; 
   filter: alpha(opacity=30); 
   text-align: center; 
   line-height: 30px; 
   font-size: 20px; 
   height: 30px; 
   width: 30px; 
   background-color: #fff; 
   float: left; 
   border-radius:3px; 
   cursor:pointer; 
  }
  
#slider 
  { 
   position: absolute; 
   top: 0px; 
   left: 0px; 
  }
  #slider img 
  { 
   float: left; 
   border: none; 
  } 


