@charset "utf-8";
/* CSS Document */
*{ margin:0px; padding:0px; border:0px;}
body{ font-family:"微软雅黑"; font-size:14px; *font-size:14px; _font-size:14px; width:100%;}
.box{ width:100%; margin:0 auto; }
.clear{ clear:both;}
a:link, a:visited{ color:#000; text-decoration:none;}
a:hover{ color:#9b0b0d;}
ul{ list-style:none;}

/*........top..........*/
.top{ width:100%;height:40px;background:url(images/tou.png) no-repeat; float:left; text-align:center; }
