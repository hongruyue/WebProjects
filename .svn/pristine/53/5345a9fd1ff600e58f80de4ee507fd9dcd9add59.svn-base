<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title></title>
</head>
<body>
<div></div>
</body>
<script src="js/jquery-1.8.3.min.js"></script>
<script>
    $(function(){
        $("button").click(function(){
            $.ajax({
                url:"http://freegeoip.net/json/?callback=&wd=",
                type:"get",
                dataType:"json",
                data:"",//post提交，写是data后面
                success:function(data){
                    $("div").html(data.ip)
                    alert(data)
                },
                error:function(){

                },
                asyns:true
            })
        })
    })
</script>
</html>