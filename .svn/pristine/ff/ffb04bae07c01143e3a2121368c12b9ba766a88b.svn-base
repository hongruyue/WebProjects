	function bubbleSort(arr){
		var i,j,temp;
		for(i=arr.length;i>0;i--){
			for(j=0;j<i-1;j++){
				if(arr[j]>arr[j+1]){
					temp=arr[j];
					arr[j]=arr[j+1];
					arr[j+1]=temp;
				}
			}
			
		}
		document.write(arr);
	}
	
	bubbleSort(arr);
	
