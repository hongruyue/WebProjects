<script>
    $(function(){
        $.ajax({
            url:"news",
            type:"get",
            dataType:"json",
            success:function(data){
                for(var i=0;i<data.length;i++){
                    var lis="<li><a href='content.html#code="+data[i].code+"'>"+data[i].title+"</a></li>"
                    $("ul").append(lis);
                }
            },
            error:function(){

            },
            async:true
        })

