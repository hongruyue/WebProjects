/*清除浮动*/
.qingfu{
    clear: both;
}
body{
    overflow-y: hidden;
}

/*页眉部分*/
.headers{
    width: 100%;
    height: 60px;
    background: #FFFFFF;
}
.header{
    width: 1200px;
    height: 60px;
    background: #FFFFFF;
    margin: 0 auto;
}
.header a{
    color: #414A60;
}
.logo a{
    width: 170px;
    height: 24px;
    background: url("../img/logo-x.png") 0 0 no-repeat;
    float: left;
    margin-top: 17px;
    margin-right: 32px;
}
.hdzuo{
    float: left;
}
.hdzuo .app{
    color: #55CBC4;
}
.hdzuo li{
    float: left;
    line-height: 60px;
    margin-right: 34px;
}
.hdyou{
    float: right;
}
.hdyou li{
    float: left;
    line-height: 60px;
}

.hdyou .hd_zhc{
    height: 24px;
    line-height: 24px;
    color: #9FA3B0;
    padding: 0 10px 0 12px;
    display: inline-block;
    border: 1px solid #5DD5C8;
    border-radius: 15px;
    margin-left: 37px;
    transition: all 1s ease;
}
.hdyou .hd_zhc:hover{
    background: #5DD5C8;
    color: #FFFFFF;
}
.hdyou .hd_dl{
    height: 24px;
    line-height: 24px;
    color: #9FA3B0;
    padding: 0 10px 0 12px;
    display: inline-block;
    border: 1px solid #5DD5C8;
    border-radius: 15px;
    margin-left: 17px;
    transition: all 1s ease;
}
.hdyou .hd_dl:hover{
    background: #5DD5C8;
    color: #FFFFFF;
    /*animation: zhuce 1s ;*/
}
/*内容*/
/*右边的浮动下标*/
.youfu{
    position: absolute;
    right: 40px;
    z-index: 10;
}
.youfu li{
    width: 10px;
    height: 10px;
    background:green ;
    border-radius: 50%;
    margin-bottom: 20px;
    cursor: pointer;
    transition: all 1s ease;
}
.youfu li:hover{
    background:yellow ;
    transform: scale(1.8);
}

