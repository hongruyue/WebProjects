	function bubbleSort(arr){
		var len=arr.length,j;
		var temp;
		while(len>0){
			for(j=0;j<len-1;j++){
				if(arr[j]>arr[j+1]){
					temp=arr[j];
					arr[j]=arr[j+1];
					arr[j+1]=temp;
				}
			}
			len--;
		}
		document.write(arr);
	}
	
	bubbleSort(arr);
	
