}

.navp4 {
    width: 75px;
    height: 75px;
    background: url("../img/3_03.png") no-repeat;
}

.nav1 li:nth-child(5) a:hover .navp4 {
    background: url("../img/11_03.png") no-repeat;
}

.navp5 {
    width: 75px;
    height: 75px;
    background: url("../img/4_03.png") no-repeat;
}

.nav li:nth-child(6) a:hover .navp5 {
    background: url("../img/12_03.png") no-repeat;
