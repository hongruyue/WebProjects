/*头部*/
.header {
    width: 1200px;
    margin: 0 auto;
}

.header_box {
    height: 120px;
}

.header_left {
    width: 126px;
    height: 70px;
    display: inline-block;
    float: left;
    cursor: pointer;
    background: url("../img/logo_new.png") 0 0 no-repeat;
    margin-top: 20px;
    margin-left: 100px;
}

.header_right {
    width: 340px;
    height: 40px;
    display: inline-block;
    float: right;
    cursor: pointer;
    background: url("../img/head_three_icon.png") 0 0 no-repeat;
    margin-top: 35px;
    margin-right: 100px;
}

/*关于折800*/
.gy {
    width: 100%;
    height: 650px;
    background: url("../img/aboutus_pic_1_w.jpg") center 0 no-repeat;
}

/*<!--nav start-->*/
.nav{
    width: 1200px;
    height: 150px;
    margin: 0 auto;

}
.nav1 {
    width: 1200px;
    height: 150px;
    margin: 0 auto;
    background: #FFFFFF;
    border-bottom: 1px solid #ccc;
}

.nav1 li {
    width: 75px;
    height: 90px;
    float: left;
    margin-left: 85px;
    margin-top: 30px;
}

.nav1 li:nth-child(1) a:hover .navp {
    background: url("../img/7_03.png") no-repeat;
}

.nav1 li a:hover p {
    color: #e50043;
}

.navp {
    width: 75px;
    height: 75px;
    background: url("../img/about_us_icon_03.png") no-repeat;
}

.nav1 p {
    font-size: 15px;
    padding-top: 5px;
    padding-left: 10px;
    color: black;
}

.navp1 {
    width: 75px;
    height: 75px;
    background: url("../img/about_us_icon_03%20(2).png") no-repeat;
}

.nav1 li:nth-child(2) a:hover .navp1 {
    background: url("../img/8_03.png") no-repeat;
}

.navp2 {
    width: 75px;
    height: 75px;
    background: url("../img/1_03.png") no-repeat;
}

.nav1 li:nth-child(3) a:hover .navp2 {
